<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.h2net.goldsafe.mapper.UserMapper">
    <insert id="insertEntry" parameterType="com.h2net.goldsafe.user.UserVo">
        INSERT INTO entry (
                           name
                          , entry_number
                          , entry_time)
        VALUES (
                #{entryName}
               , #{entryNumber}
               , now())
    </insert>

</mapper>